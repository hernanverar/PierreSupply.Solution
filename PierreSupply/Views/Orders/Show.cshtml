@{
  Layout = "_Layout";
}
<div>
<h1> Vendor Order Details:</h1>

<h2>@Model["order"].Title</h2>
<h3>Vendor: @Model["vendor"].Name</h3>
<h2>Description: @Model["order"].Description</h2>
<h2>Price: $@Model["order"].Price</h2>
<h2>Ordered on: @Model["order"].Date</h2>


<a href='/vendors/@Model["vendor"].Id/orders/new'>Will you like to place another Order?</a>
<a href='/vendors'>Back to Vendor </a>
<a href='/'>Back to Home Page</a>
</div>
